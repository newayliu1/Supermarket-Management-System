<h1>Table</h1>
<table class="table">
  <thead>
    <tr>
      <th>Product ID</th>
      <th>Name</th>
      <th>Sections</th>
      <th>Inventories ID</th>
      <th>Orders ID</th>
    </tr>
  </thead>
  <tbody>
    {{#each products as |product|}}
    <tr>
      <td data-id="{{product.id}}">{{product.id}}</td>
      <td>{{product.name}}</td>
      <td>{{product.sections}}</td>
      <td>{{product.inventoriesID}}</td>
      <td>{{product.ordersID}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>
