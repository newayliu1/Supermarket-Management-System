<h1>Table</h1>
<table class="table">
  <thead>
    <tr>
      <th>Order ID</th>
      <th>Product</th>
      <th>Amount</th>
      <th>Expiration Date</th>
      <th>Order Date</th>
    </tr>
  </thead>
  <tbody>
    {{#each orders as |order|}}
    <tr>
      <td data-id="{{order.id}}">{{order.id}}</td>
      <td>{{order.product.name}}</td>
      <td>{{order.amount}}</td>
      <td>{{order.expiration_date}}</td>
      <td>{{order.order_date}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>
